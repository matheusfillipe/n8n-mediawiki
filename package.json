{
  "name": "n8n-nodes-mediawiki",
  "version": "0.2.0",
  "description": "n8n community node for MediaWiki integration",
  "keywords": [
    "n8n-community-node-package",
    "mediawiki",
    "wiki"
  ],
  "license": "MIT",
  "homepage": "https://github.com/matheusfillipe/n8n-mediawiki",
  "author": {
    "name": "matheusfillipe",
    "email": "mattf@h4ks.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheusfillipe/n8n-mediawiki"
  },
  "bugs": {
    "url": "https://github.com/matheusfillipe/n8n-mediawiki/issues"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run lint && npm run test",
    "version": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MediaWikiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MediaWikiPage/MediaWikiPage.node.js",
      "dist/nodes/MediaWikiSearch/MediaWikiSearch.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^29.5.0",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.5.0",
    "n8n-workflow": "*",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.0",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@langchain/core": "^0.1.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
