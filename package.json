{
  "name": "n8n-nodes-mediawiki",
  "version": "0.1.1",
  "description": "n8n community node for MediaWiki integration",
  "keywords": [
    "n8n-community-node-package",
    "mediawiki",
    "wiki"
  ],
  "license": "MIT",
  "homepage": "https://github.com/h4ks/n8n-mediawiki#readme",
  "author": {
    "name": "H4ks",
    "email": "hello@h4ks.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h4ks/n8n-mediawiki.git"
  },
  "bugs": {
    "url": "https://github.com/h4ks/n8n-mediawiki/issues"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "version": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MediaWikiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MediaWiki/MediaWiki.node.js",
      "dist/nodes/MediaWikiPageTool/MediaWikiPageTool.node.js",
      "dist/nodes/MediaWikiSearchTool/MediaWikiSearchTool.node.js",
      "dist/nodes/MediaWikiCreatePageTool/MediaWikiCreatePageTool.node.js",
      "dist/nodes/MediaWikiUpdatePageTool/MediaWikiUpdatePageTool.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.1.1",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@langchain/core": "^0.1.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
